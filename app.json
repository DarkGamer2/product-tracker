        "package": "com.yourcompany.yourappname",
        "versionCode": 1
      },
      "package": "com.kameer_17.producttracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Roboto-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9e42c5df-cdf0-4317-870c-022013f61efb"
      }
    }
